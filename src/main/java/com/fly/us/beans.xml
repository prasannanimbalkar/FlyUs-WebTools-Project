<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	<!-- bean definitions go here -->
	<bean id="flightValidator" class="com.fly.us.validator.FlightBookingValidator"></bean>
	<bean id="airplaneValidator" class="com.fly.us.validator.AirlineValidator"></bean>
	<bean id="passengerValidator" class="com.fly.us.validator.TravellerValidator"></bean>
	<bean id="paymentValidator" class="com.fly.us.validator.PaymentProcessValidator"></bean>

</beans>